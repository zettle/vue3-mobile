{{#spritesheet}}
.bank-logo-icon {
    display: inline-block;
    background-image: url({{{escaped_image}}});
    background-size: {{px.width}} {{px.height}};
}
{{/spritesheet}}
{{#sprites}}

.bank-logo-icon-{{name}} {
    width: {{px.width}};
    height: {{px.height}};
    background-position: {{px.offset_x}} {{px.offset_y}};
}
{{/sprites}}