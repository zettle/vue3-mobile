{
    "name": "vue3-mobile",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "nodemon --watch vue.config.js --exec \"npm run serve\"",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "jslint": "vue-cli-service lint",
        "csslint": "stylelint ./src/**/*.{html,vue,css,sass,scss} --fix",
