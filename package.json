        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^7.0.0-0",
        "nodemon": "^2.0.6",
        "postcss-pxtorem": "^5.1.1",
        "sass": "^1.26.5",
